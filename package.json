{
    "name": "sudoku",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint . --ext .tsx,.ts",
        "test": "jest",
        "prepare": "husky install",
        "commit": "git-cz",
        "lint:fix": "eslint . --ext .tsx,.ts --fix"
    },
    "dependencies": {
        "@prisma/client": "^4.15.0",
        "@types/react": "18.2.8",
        "@types/react-dom": "18.2.4",
        "autoprefixer": "10.4.14",
        "eslint": "8.42.0",
        "eslint-config-next": "13.4.4",
        "next": "13.4.4",
        "pg": "^8.11.0",
        "postcss": "8.4.31",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "tailwindcss": "3.3.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.5",
        "@commitlint/config-conventional": "^17.6.5",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.2.5",
        "commitizen": "^4.3.0",
        "husky": "^8.0.0",
        "jest": "^29.5.0",
        "prisma": "^4.15.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm test"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
